<script src="~/js/Order/MyOrder.js?Date=123"></script>

 <head>
    <link rel="stylesheet" href="Cart.css" />
    <!-- CSS only -->
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx"
      crossorigin="anonymous"
    />
  </head>
  <body>
  

    <main class="p-4">
      <div class="container d-flex flex-direction-row gap-4">
        <div class="item-wrapper" id=divTempOrders>
      
        </div>
       
      </div>
         <div class="modal fade" id="modalDetail" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Alışverişi Tamamla</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="col-4">
                            <div class="input-group mb-2">
                                <label for="basic-url">Adres</label>
                            </div>
                        </div>
                        <div class="col-8">
                            <div class="input-group mb-4">
                                <textarea type="text" class="form-control" id="txt_Address" placeholder="Adres" aria-label="Username" aria-describedby="basic-addon1"></textarea>
                            </div>
                        </div>
                    </div>
                
                    <div class="row">
                        <div class="col-4">
                            <div class="input-group mb-2">
                                <label for="basic-url">Kredi Kartı Bilgisi</label>
                            </div>
                        </div>
                        <div class="col-8">
                            <div class="input-group mb-4">
                                <input onkeypress="return onlyNumber(event)" maxlength="16" class="form-control" id="txt_CreditCard" placeholder="Kredi Kartı" aria-label="Username" aria-describedby="basic-addon1">
                            </div>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Kapat</button>
                    <button type="button" class="btn btn-primary" onclick="SaveDetail()">Kaydet</button>
                </div>
            </div>
        </div>
    </div>
    </main>
  </body>